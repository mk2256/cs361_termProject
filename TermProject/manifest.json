{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
      "notifications", "activeTab", "downloads", "storage"
    ],
    "background": {
      "service_worker": "backgroundScript.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "hello_extensions.jpg"
    }
  }
